#include <stdio.h>

/**
* print_S - prints a string
* @str: pointer to char to be printed
* Return: nothing, prints a string or hex value of non-printable chars
**/
void print_S(char *str)
{
	int i, j;

	for (i = 0; str[i] != '\0'; i++)
	{
		j = (int)str[i];
		if (j < 32 || j >= 127)
			printf("%d\n", j);
	}
}

void main()
{
	print_S("" NULSOH);
}
